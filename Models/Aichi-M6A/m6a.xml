    <object-name>m6a-aileronG</object-name>
    <object-name>m6a-voletG1</object-name>
    <object-name>m6a-voletG2</object-name>
    <object-name>m6a-aileronD</object-name>
    <object-name>m6a-voletD1</object-name>
    <object-name>m6a-voletD2</object-name>
    <object-name>m6a-ailes2</object-name>
    <object-name>m6a-direction</object-name>
    <object-name>m6a-profondeur</object-name>
    <object-name>m6a-bol</object-name>
    <object-name>m6a-antenne</object-name>
    <object-name>m6a-supports</object-name>
    <object-name>m6a-gouvernailG</object-name>
