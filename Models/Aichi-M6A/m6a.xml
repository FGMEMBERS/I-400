  <effect>
    <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
    <object-name>m6a-fuselage</object-name>
    <object-name>m6a-verriereA</object-name>
    <object-name>m6a-verriereB</object-name>
    <object-name>m6a-ailes</object-name>
    <object-name>m6a-aileronG</object-name>
    <object-name>m6a-voletG1</object-name>
    <object-name>m6a-voletG2</object-name>
